extends layout
block content
      .maincontent
        
          h2 Here is a sample of the muscles you will have to study
            .grid-container
              each row in res1
                .item
                  h1.itemtitle #{row.Name}
                  label origin: 
                  p.itemp #{row.Origin}
                  label insertion: 
                  p.itemp #{row.Insertion}
                  label action: 
                  p.itemp #{row.Action}
                  a.button(href="/muscle/#{row.Id}") show
        